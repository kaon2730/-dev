import java.util.Scanner;

public class Anbghjh {
		    public static void main(String[] args) {
		    	double armutfiyati = 2.14;
		    	double elmafiyati = 3.67;
		    	double domatesfiyati = 1.11;
		    	double muzfiyati = 0.95;
		    	double patlicanfiyati= 5.0;
		    	double toplam=0.0;
		    	Scanner scan= new Scanner(System.in);
		    	System.out.println("Armut kac kilo?");
		    	toplam += armutfiyati*scan.nextInt();
		    	System.out.println("Elma kac kilo?");
		    	toplam += elmafiyati * scan.nextInt();
		    	System.out.println("Domates kac kilo?");
		    	toplam+= domatesfiyati*scan.nextInt();
		    	System.out.println("Muz kac kilo?");
		    	toplam += muzfiyati* scan.nextInt();
		    	System.out.println("Patlican kac kilo");
		    	toplam += patlicanfiyati*scan.nextInt();
		    	System.out.println("Toplam tutar : " + toplam);
		    }		    	  	
}
